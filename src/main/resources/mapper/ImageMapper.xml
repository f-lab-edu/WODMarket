<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.flab.wodmarket.domain.image.ImageMapper">

    <insert id="save" parameterType="com.flab.wodmarket.domain.image.dto.request.ImageInfo">
        INSERT INTO itemImg
        (item_no,
         imgOrgName,
         imgName,
         img_url,
         seq,
         reg_date,
         mod_date)
        VALUES (#{itemNo},
                #{imgOrgName},
                #{imgName},
                #{url},
                #{seq},
                now(),
                now())
    </insert>


</mapper>